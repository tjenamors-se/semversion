  "version": "1.0.1",
  "autoload": {
    "psr-4": {
      "Tjenamors\\Semversion\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Magnus Ã…berg",
      "email": "himself@magnusaberg.me"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.3"
  },
